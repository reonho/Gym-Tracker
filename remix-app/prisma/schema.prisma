generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model exercises {
  id      String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  history Json[]
  name    String
}

model workouts {
  id String @id @default(dbgenerated()) @map("_id") @db.ObjectId
}
